  <Width>18.48645cm</Width>
  <Sections>
    <PageHeader Name="pageHeaderSection1">
      <Height>0.12cm</Height>
      <Variables>
        <ReportVariable>
          <Name>vCompanyName</Name>
          <ValueExpr>=[BAccount.AcctName]</ValueExpr>
        </ReportVariable>
      </Variables>
    </PageHeader>
    <Detail Name="detailSection1">
      <GridSize>4px, 4px</GridSize>
      <Height>0.53492cm</Height>
      <ProcessOrder>Always</ProcessOrder>
      <Variables>
        <ReportVariable>
          <Name>LineAmount</Name>
          <ProcessOrder>WhilePrint</ProcessOrder>
          <ValueExpr>=IIf([ARStatement.StatementType]=[@TypeOpenItem],[ARStatementDetailInfo.DocBalanceSigned],[ARStatementDetailInfo.OrigDocAmtSigned])</ValueExpr>
        </ReportVariable>
        <ReportVariable>
          <Name>TotalBegBalance</Name>
          <ValueExpr>=IIf(Prev([ARStatement.CustomerID])&lt;&gt; [ARStatement.CustomerID],[ARStatement.BegBalance], IIF(Prev([ARStatement.CuryID])&lt;&gt;[ARStatement.CuryID],$TotalBegBalance +[ARStatement.BegBalance],$TotalBegBalance))</ValueExpr>
        </ReportVariable>
        <ReportVariable>
          <Name>LineBalance</Name>
          <ProcessOrder>WhilePrint</ProcessOrder>
          <ResetGroup>groupBranch</ResetGroup>
          <ValueExpr>=iif($LineBalance&lt;&gt;null,$LineBalance,0) + iif($LineAmount&lt;&gt;null,$LineAmount,0)</ValueExpr>
        </ReportVariable>
        <ReportVariable>
          <Name>ConsolLineBalance</Name>
          <ProcessOrder>WhilePrint</ProcessOrder>
          <ResetGroup>groupCustomer</ResetGroup>
          <ValueExpr>=$ConsolLineBalance + $LineAmount</ValueExpr>
        </ReportVariable>
        <ReportVariable>
          <Name>TotalAgeBalance00</Name>
          <ValueExpr>=IIf(Prev([ARStatement.CustomerID])&lt;&gt; [ARStatement.CustomerID],[ARStatement.AgeBalance00], IIF(Prev([ARStatement.CuryID])&lt;&gt;[ARStatement.CuryID],$TotalAgeBalance00 +[ARStatement.AgeBalance00],$TotalAgeBalance00))</ValueExpr>
        </ReportVariable>
        <ReportVariable>
          <Name>TotalAgeBalance01</Name>
          <ValueExpr>=IIf(Prev([ARStatement.CustomerID])&lt;&gt; [ARStatement.CustomerID],[ARStatement.AgeBalance01], IIF(Prev([ARStatement.CuryID])&lt;&gt;[ARStatement.CuryID],$TotalAgeBalance01 +[ARStatement.AgeBalance01],$TotalAgeBalance01))</ValueExpr>
        </ReportVariable>
        <ReportVariable>
          <Name>TotalAgeBalance02</Name>
          <ValueExpr>=IIf(Prev([ARStatement.CustomerID])&lt;&gt; [ARStatement.CustomerID],[ARStatement.AgeBalance02], IIF(Prev([ARStatement.CuryID])&lt;&gt;[ARStatement.CuryID],$TotalAgeBalance02 +[ARStatement.AgeBalance02],$TotalAgeBalance02))</ValueExpr>
        </ReportVariable>
        <ReportVariable>
          <Name>TotalAgeBalance03</Name>
          <ValueExpr>=IIf(Prev([ARStatement.CustomerID])&lt;&gt; [ARStatement.CustomerID],[ARStatement.AgeBalance03], IIF(Prev([ARStatement.CuryID])&lt;&gt;[ARStatement.CuryID],$TotalAgeBalance03 +[ARStatement.AgeBalance03],$TotalAgeBalance03))</ValueExpr>
        </ReportVariable>
        <ReportVariable>
          <Name>TotalAgeBalance04</Name>
          <ValueExpr>=IIf(Prev([ARStatement.CustomerID])&lt;&gt; [ARStatement.CustomerID],[ARStatement.AgeBalance04], IIF(Prev([ARStatement.CuryID])&lt;&gt;[ARStatement.CuryID],$TotalAgeBalance04 +[ARStatement.AgeBalance04],$TotalAgeBalance04))</ValueExpr>
        </ReportVariable>
      </Variables>
      <VisibleExpr>=IIf([ARStatementDetailInfo.RefNbr]&lt;&gt;null, true,false)</VisibleExpr>
      <Items>
        <TextBox Name="textBox22">
          <CanGrow>True</CanGrow>
          <Location>312px, 4px</Location>
          <Size>88px, 16px</Size>
          <Style>
            <TextAlign>Left</TextAlign>
          </Style>
